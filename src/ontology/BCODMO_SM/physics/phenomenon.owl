        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#physics"/>
        <rdfs:label xml:lang="en">oceanic sea surface microlayer</rdfs:label>
        <rdfs:label>oceanic sea surface microlayer biome</rdfs:label>
    </owl:Class>
