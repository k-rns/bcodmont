    <!-- http://purl.obolibrary.org/obo/ENVO_01001706 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/ENVO_01001706">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01001681"/>
        <obo:IAO_0000115 xml:lang="en">A weather front which is nonoccluded and  which moves in the direction of the warmer air mass it is adjacent to, such that colder air will occupy the site where warmer air was previously present.</obo:IAO_0000115>
        <dc:creator rdf:resource="http://orcid.org/0000-0002-4366-3088"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-08-20T16:53:00Z</dc:date>
        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#context"/>
        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#matrix"/>
        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#phenomenon"/>
        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#physics"/>
        <oboInOwl:inSubset>envoAtmo</oboInOwl:inSubset>
        <rdfs:comment xml:lang="en">The direction of motion determines whether fronts are &quot;cold&quot;, &quot;warm&quot;, etc. A front is said to be cold if it&apos;s moving in the direction of the warmer air mass it is adjacent to. From the perspective of an observer on the ground, this appears as though the colder air &quot;replaces&quot; the warmer air mass.</rdfs:comment>
        <rdfs:label xml:lang="en">cold front</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.obolibrary.org/obo/ENVO_01001706"/>
        <owl:annotatedProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000115"/>
