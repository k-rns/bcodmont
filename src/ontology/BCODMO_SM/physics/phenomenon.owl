        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#physics"/>
        <rdfs:label xml:lang="en">marine eddy</rdfs:label>
        <rdfs:label>mesoscale marine eddy</rdfs:label>
        <rdfs:label xml:lang="en">mesoscale marine eddy</rdfs:label>
    </owl:Class>
