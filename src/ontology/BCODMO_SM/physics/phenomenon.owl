        <oboInOwl:inSubset rdf:resource="http://bcodmo/sm#physics"/>
        <rdfs:label xml:lang="en">neritic sea surface microlayer</rdfs:label>
        <rdfs:label>neritic sea surface microlayer biome</rdfs:label>
    </owl:Class>
